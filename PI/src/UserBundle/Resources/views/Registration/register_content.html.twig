{% trans_default_domain 'FOSUserBundle' %}
                            <script src="{{ asset('js/jquery-3.2.1.js') }}"></script>
                            <script src="{{ asset('js/RegisterUser.js') }}"></script>
                            <div class="tab-content">
                                <div class="tab-pane fade in active" id="tab1">
                                    {{ form_start(form, {'method': 'post', 'action': path('fos_user_registration_register'), 'attr': {'class': 'fos_user_registration_register'}}) }}

                                    <div class="form-group">
                                            <label>Nom d'utilisateur : <span class="noempaty">*</span></label>
                                            {{ form_widget(form.username,{'attr':{'class':'form-control','placeholder':"Saisir votre nom d'utilisateur" }}) }}
                                            {{ form_errors(form.username) }}
                                        </div>
                                        <div class="form-group">
                                            <label>Nom : <span class="noempaty">*</span></label>
                                            {{ form_widget(form.nom,{'attr':{'class':'form-control','placeholder':'Saisir votre nom '}}) }}
                                            {{ form_errors(form.nom) }}
                                        </div>
                                        <div class="form-group">
                                            <label>Prenom : <span class="noempaty">*</span></label>
                                            {{ form_widget(form.prenom,{'attr':{'class':'form-control','placeholder':"Saisir votre prenom "}}) }}
                                            {{ form_errors(form.prenom) }}
                                        </div>
                                        <div class="form-group">
                                            <label>Date de Naissance : <span class="noempaty">*</span></label>
                                            {{ form_widget(form.d_naiss,{'attr':{'class':'form-control','placeholder':"Date de naissance "}}) }}
                                            {{ form_errors(form.d_naiss) }}
                                        </div>
                                        <div class="form-group">
                                            <label>Email : <span class="noempaty">*</span></label>
                                            {{ form_widget(form.email,{'attr':{'class':'form-control','placeholder':"Saisir votre email "}}) }}
                                            {{ form_errors(form.email) }}
                                        </div>
                                        <div class="form-group">
                                            <label>Password : <span class="noempaty">*</span></label>
                                            {{ form_widget(form.plainPassword.first,{'attr':{'class':'form-control','placeholder':"Saisir votre password "}}) }}
                                            {{ form_errors(form.plainPassword.first) }}
                                        </div>
                                        <div class="form-group">
                                            <label>Confirm Password : <span class="noempaty">*</span></label>
                                            {{ form_widget(form.plainPassword.second,{'attr':{'class':'form-control','placeholder':"Retaper votre password "}}) }}
                                            {{ form_errors(form.plainPassword.second) }}
                                        </div>
                                        <div class="form-group">
                                            <label>Adresse : <span class="noempaty">*</span></label>
                                            {{ form_widget(form.adresse,{'attr':{'class':'form-control','placeholder':"Saisir votre adresse "}}) }}
                                            {{ form_errors(form.adresse) }}
                                        </div>
                                        <div class="form-group">
                                            <label>Num Tel : <span class="noempaty">*</span></label>
                                            {{ form_widget(form.numTel,{'attr':{'class':'form-control','placeholder':"Saisir votre num tel "}}) }}
                                            {{ form_errors(form.numTel) }}
                                        </div>

                                        <div id="Prest">

                                        <div class="form-group">
                                            <label>Fonction : <span class="noempaty">*</span></label>
                                            {{ form_widget(form.fonction,{'attr':{'class':'form-control','placeholder':"Saisir votre Fonction "}}) }}
                                            {{ form_errors(form.fonction) }}
                                        </div>

                                        <div class="form-group" id="spec">
                                            <label>Specialite : <span class="noempaty">*</span></label>
                                            {{ form_widget(form.specialite,{'attr':{'class':'form-control','placeholder':"Saisir votre Specialit√© "}}) }}
                                            {{ form_errors(form.specialite) }}
                                        </div>

                                        </div>

                                        <div class="form-group">
                                            <div class="checkbox">
                                                <label>
                                                    <input id="Cb" type="checkbox"><span>Vous etes un Prestataire ?</span><br>
                                                    <input type="checkbox"> Don't subscribe our newslatter
                                                </label>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <button type="submit" class="btn btn-primary" value="{{ 'registration.submit'|trans }}">Register now</button>
                                        </div>


                                    {{ form_widget(form) }}

                                    {{ form_end(form) }}

                                </div>
                                <div class="tab-pane fade" id="tab2">
                                    <div class="row">
                                        <div class="col-md-12 text-center marginbot20">
                                            <h5>Login with social media account</h5>
                                            <a href="#" class="btn btn-primary btn-icon btn-block btn-picton btn-lg"><i class="fa fa-facebook"></i> Facebook</a>
                                            <a href="#" class="btn btn-primary btn-icon btn-block btn-turquoise btn-lg"><i class="fa fa-twitter"></i> Twitter</a>
                                            <a href="#" class="btn btn-primary btn-icon btn-block btn-serria btn-lg"><i class="fa fa-instagram"></i> Instagram</a>
                                        </div>
                                    </div>
                                </div>
                            </div>










